<addon id="repository.torrentbr" name="repository.torrentbr" version="0.0.1" provider-name="kreatorkodi">
	<extension point="xbmc.addon.repository" name="TorrentBR">
		<info compressed="false">http://github.com/kreatorkodi/repository.torrentbr/master/addons.xml</info>
		<checksum>http://github.com/kreatorkodi/repository.torrentbr/master/addons.xml.md5</checksum>
		<datadir zip="true">http://github.com/kreatorkodi/repository.torrentbr/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>TorrentBR</summary>
		<description>Addon Description</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
